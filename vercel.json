{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["prisma/**"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ],
  "installCommand": "npm install && npx prisma generate",
  "env": {
    "PRISMA_CLI_BINARY_TARGETS": "rhel-openssl-1.0.x"
  }
}